{
    "manifest_version": 3,
    "name": "GPTEnhancer",
    "description": "Provides you the feature to upload files to ChatGPT and make it more user specific",
    "version": "1.0.2",
    "icons":{
        "16":"icon16.png",
        "48":"icon48.png",
        "96":"icon96.png",
        "128":"icon128.png",
        "192":"icon192.png"
    },
    "action":{
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "96": "icon96.png",
            "128":"icon128.png",
            "192":"icon192.png"
        },
        "default_title":"GPTEnhancer: Upload file and boost GPT experience"
    },
    "content_scripts": [
        {
            "js":["new_content.js", "pdf.min.js", "pdf.worker.min.js", "tesseract.min.js"],
            "matches":["https://chat.openai.com/*"]
        }
    ]
}


